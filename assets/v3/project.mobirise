\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#149dcc\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row align-items-end\" mbr-class=\"{'flex-row-reverse' : reverse}\">\n            <div class=\"col-lg-5 content__block\">\n                <div class=\"countdown-cont \">\n                    <div class=\"countdown\" data-due-date=\"{{countdown}}\"></div>\n                </div>\n                <div mbr-class=\"{'align-left' : alignCountdown == 1, 'align-center' : alignCountdown == 2, 'align-right' : alignCountdown == 3}\">\n                    <span mbr-text class=\"span__title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".span__title\">Starts in</span>\n                    <h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">Welcome to attend the annual conference</h2>\n                </div>\n            </div>\n\n\n            <div class=\"col-lg-7\">\n                <div class=\"image\">\n                    <img src=\"../_images/conference16.jpg\" alt title>\n                    <div class=\"mbr-figure\">\n                        <div class=\"mbr-media show-modal align-center\" data-modal=\"