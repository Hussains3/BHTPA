          "_customHTML": "<section class=\"footer1 modernm4_footer1\" group=\"Footer\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" always-bottom global once=\"footer\" not-draggable>\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Show Animation\" name=\"showAnimation\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Brand\" name=\"showBrand\" checked>\n        <select name=\"typeMenu\" title=\"Type Menu\" condition=\"showBrand\">\n            <option value=\"1\" selected>Logo Text</option>\n            <option value=\"2\">Logo Image</option>\n        </select>\n        <input type=\"range\" inline title=\"Width Logo\" name=\"widthLogo\" min=\"20\" max=\"100\" step=\"2\" value=\"60\" condition=\"showBrand && typeMenu == 2\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"range\" inline title=\"Menu Items 1\" name=\"menuItems\" min=\"0\" max=\"6\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Menu Items 2\" name=\"menuItems2\" min=\"0\" max=\"6\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Social Icons\" name=\"socIcons\" min=\"0\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"color\" title=\"Icon Color\" value=\"#dbdbdb\" name=\"iconColor\" condition=\"socIcons > 0\">\n        <hr>\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\" checked>\n        <input type=\"color\" title=\"Divider Color\" value=\"#000000\" name=\"dividerColor\" condition=\"showBottom\">\n        <input type=\"range\" inline title=\"Menu Bottom Items\" name=\"menuBottomItems\" min=\"0\" max=\"6\" step=\"1\" value=\"3\" condition=\"showBottom\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"../_images/background/backgraund__trend.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#fbfcff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <img src=\"../_images/figure/triangle_purpl_semitransp.svg\" class=\"triangle\" alt title>\n        <div class=\"circle\"></div>\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap order-0\" mbr-if=\"showBrand\">\n                <p class=\"brand__text mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"typeMenu == 1\" data-app-selector=\".brand__text\"><a href=\"#\" class=\"mbr-bold text-primary\">ModernM4</a></p>\n                <img class=\"logo__image\" mbr-if=\"typeMenu == 2\" src=\"../_images/logo2.png\" alt=\"Mobirise\" title>\n                <p class=\"brand__email mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".brand__email\"><a href=\"#\" class=\"text-secondary\">mobirise@company.com</a></p>\n                <p class=\"brand__phone mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".brand__phone\"><a href=\"#\" class=\"text-danger\">Ph: +(123)456-78-90</a></p>\n            </div>\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap\">\n                <h4 class=\"mbr-fonts-style mbr-regular footer__title\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".footer__title\">About us</h4>\n                <div class=\"mbr-list\" mbr-list mbr-list-grow=\"menuItems\" mbr-if=\"menuItems > 0\">\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"index.html\" class=\"text-secondary\">Features</a></p>\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"index.html\" class=\"text-secondary\">Templates</a></p>\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"index.html\" class=\"text-secondary\">Integrations</a>\n                    </p>\n                </div>\n            </div>\n            <div class=\"col-lg-3 col-sm-6 col-12 wrap\">\n                <h4 class=\"mbr-fonts-style mbr-regular footer__title\" mbr-if=\"showTitle\" mbr-theme-style=\"display-5\" data-app-selector=\".footer__title\">Services</h4>\n                <div class=\"mbr-list\" mbr-list mbr-list-grow=\"menuItems2\" mbr-if=\"menuItems2 > 0\">\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"index.html\" class=\"text-secondary\">Support</a></p>\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".footer__items\"><a href=\"index.html\" class=\"text-secondary\">Help Center</a>\n                    </p>\n                    <p class=\"footer__items mbr-fonts-style\" mbr-theme-sty